{
    "id": "mattermost-google-meet-plugin",
    "version": "0.1.0",
    "name": "Google Meet Plugin",
    "description": "Start Google Meet calls",
    "homepage_url": "https://github.com/SrMouraSilva/google-meet-plugin",
    "support_url": "https://github.com/SrMouraSilva/google-meet-plugin/issues",
    "min_server_version": "5.26.0",
    "icon_path": "assets/starter-template-icon.svg",
    "webapp": {
        "bundle_path": "webapp/dist/main.js"
    },
    "settings_schema": {
        "header": "Google Meet Plugin configurations",
        "settings": [{
            "key": "OpenAutomatically",
            "display_name": "Open a new tab automatically",
            "type": "bool",
            "help_text": "When true, a new tab will be oppened automatically when a user click to start a new meet",
            "default": "false"
        }]
    }
}
